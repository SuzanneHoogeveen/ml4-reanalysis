@article{bishop2009ethical,
  title = {Ethical {{Sharing}} and {{Reuse}} of {{Qualitative Data}}},
  author = {Bishop, Libby},
  year = {2009},
  journal = {Australian Journal of Social Issues},
  volume = {44},
  number = {3},
  pages = {255--272},
  issn = {1839-4655},
  doi = {10.1002/j.1839-4655.2009.tb00145.x},
  abstract = {The objective of this paper is to attempt to move beyond the impasse of ethical objections to reusing qualitative data. In doing so, there is no intention of dismissing the importance of ethical debates, in fact, quite the opposite. The debate about ethical reuse needs to be deepened and broadened. First, the current terrain of research ethics will be summarised and situated in the context of broader philosophical ethical frameworks. In contrast, the debates around ethics of archiving have often been narrowly focused on participants' rights. The framework of debate should be broadened first by recognising other entities traditionally deemed within the scope of research ethics, namely the scholarly community and the public. The second useful broadening of the framework is provided by a deontological ethical stance with its focus on duties. In the final section, this expanded framework will be used to rebut several common ethical arguments against archiving qualitative data: archiving violates confidentiality; informed consent for reusing data is impossible; reusing data violates trust between researcher and participant; and, archiving creates an unacceptably high risk of data misrepresentation. If a more general philosophical debate on ethics has something to contribute, the central message must be that no single ethical claim is incontestable. The conclusion will position these debates in a wider context by asking what is at stake when the boundaries of ethical discourse about sharing data are drawn too narrowly.},
  langid = {english},
  keywords = {archive,dissertation,qualitative data,research ethics,reuse},
  file = {/Users/suzanne/Zotero/storage/VARPD7D6/Bishop - 2009 - Ethical Sharing and Reuse of Qualitative Data.pdf;/Users/suzanne/Zotero/storage/5YSJ7VHC/j.1839-4655.2009.tb00145.html}
}

