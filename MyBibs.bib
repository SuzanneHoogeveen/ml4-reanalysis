@article{forstmann2012mind,
  ids = {forstmann2012},
  title = {``{{The Mind Is Willing}}, but the {{Flesh Is Weak}}'': {{The Effects}} of {{Mind-Body Dualism}} on {{Health Behavior}}},
  shorttitle = {``{{The Mind Is Willing}}, but the {{Flesh Is Weak}}''},
  author = {Forstmann, Matthias and Burgmer, Pascal and Mussweiler, Thomas},
  year = {2012},
  month = oct,
  journal = {Psychological Science},
  volume = {23},
  number = {10},
  pages = {1239--1245},
  issn = {0956-7976, 1467-9280},
  doi = {10.1177/0956797612442392},
  abstract = {Beliefs in mind-body dualism\textemdash that is, perceiving one's mind and body as two distinct entities\textemdash are evident in virtually all human cultures. Despite their prevalence, surprisingly little is known about the psychological implications of holding such beliefs. In the research reported here, we investigated the relationship between dualistic beliefs and health behaviors.We theorized that holding dualistic beliefs leads people to perceive their body as a mere ``shell'' and, thus, to neglect it. Supporting this hypothesis, our results showed that participants who were primed with dualism reported less engagement in healthy behaviors and less positive attitudes toward such behaviors than did participants primed with physicalism. Additionally, we investigated the bidirectionality of this link.Activating health-related concepts affected participants' subsequently reported metaphysical beliefs in mind-body dualism. A final set of studies demonstrated that participants primed with dualism make real-life decisions that may ultimately compromise their physical health (e.g., consuming unhealthy food).These findings have potential implications for health interventions.},
  langid = {english},
  keywords = {dissertation}
}

@article{forstmann2015adults,
  title = {Adults Are Intuitive Mind-Body Dualists},
  author = {Forstmann, Matthias and Burgmer, Pascal},
  year = {2015},
  journal = {Journal of Experimental Psychology: General},
  volume = {144},
  number = {1},
  pages = {222--235},
  publisher = {{American Psychological Association}},
  address = {{US}},
  issn = {1939-2222(Electronic),0096-3445(Print)},
  doi = {10.1037/xge0000045},
  abstract = {In the present research, we tested the hypotheses that (a) adults are intuitive mind-body dualists, (b) that this belief can be considered a default, and (c) that it is partially explained by essentialistic reasoning about the nature of the mind. Over 8 studies, using various thought experiment paradigms, participants reliably ascribed to a physically duplicated being a greater retention of physical than of mental properties. This difference was unrelated to whether or not this being was given a proper name (Study 1b) and was only found for entities that were considered to actually possess a mind (Study 1c). Further, we found that an intuitive belief in mind-body dualism may in fact be considered a default: Taxing participants' cognitive resources (Study 2) or priming them with an intuitive (vs. analytical) thinking style (Studies 3a and 3b) both increased dualistic beliefs. In a last set of studies, we found that beliefs in mind-body dualism are indeed related to essentialistic reasoning about the mind. When a living being was reassembled from its original molecules rather than recreated from new molecules, dualistic beliefs were significantly reduced (Studies 4a and 4b). Thus, results of the present research indicate that, despite any acquired scientific knowledge about the neurological origins of mental life, most adults remain ``essentialistic mind-body dualists'' at heart. (PsycInfo Database Record (c) 2020 APA, all rights reserved)},
  keywords = {dissertation,Dualism,Intuition,Mind,Reasoning},
  file = {/Users/suzanne/Zotero/storage/F8NYMLMX/Forstmann and Burgmer - 2015 - Adults are intuitive mind-body dualists.pdf;/Users/suzanne/Zotero/storage/33UP5W6C/2014-55432-001.html;/Users/suzanne/Zotero/storage/JNLLGVWL/2014-55432-001.html}
}

