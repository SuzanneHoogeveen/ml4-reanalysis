  keywords = {Cognition,dissertation,Metaanalysis,Numeracy,Questionnaires,Reasoning,Reflection,Religion,Surveys},
  file = {/Users/suzanne/Zotero/storage/NBZ8AX7L/Pennycook et al. - 2016 - Atheists and Agnostics Are More Reflective than Re.pdf;/Users/suzanne/Zotero/storage/MAC6DL8F/article.html}
