@article{barrett2021intuitive,
  title = {Intuitive {{Dualism}} and {{Afterlife Beliefs}}: {{A Cross-Cultural Study}}},
  shorttitle = {Intuitive {{Dualism}} and {{Afterlife Beliefs}}},
  author = {Barrett, H. Clark and Bolyanatz, Alexander and Broesch, Tanya and Cohen, Emma and Froerer, Peggy and Kanovsky, Martin and Schug, Mariah G. and Laurence, Stephen},
  year = {2021},
  journal = {Cognitive Science},
  volume = {45},
  number = {6},
  pages = {e12992},
  issn = {1551-6709},
  doi = {10.1111/cogs.12992},
  abstract = {It is widely held that intuitive dualism\textemdash an implicit default mode of thought that takes minds to be separable from bodies and capable of independent existence\textemdash is a human universal. Among the findings taken to support universal intuitive dualism is a pattern of evidence in which ``psychological'' traits (knowledge, desires) are judged more likely to continue after death than bodily or ``biological'' traits (perceptual, physiological, and bodily states). Here, we present cross-cultural evidence from six study populations, including non-Western societies with diverse belief systems, that shows that while this pattern exists, the overall pattern of responses nonetheless does not support intuitive dualism in afterlife beliefs. Most responses of most participants across all cultures tested were not dualist. While our sample is in no way intended to capture the full range of human societies and afterlife beliefs, it captures a far broader range of cultures than in any prior study, and thus puts the case for afterlife beliefs as evidence for universal intuitive dualism to a strong test. Based on these findings, we suggest that while dualist thinking is a possible mode of thought enabled by evolved human psychology, such thinking does not constitute a default mode of thought. Rather, our data support what we will call intuitive materialism\textemdash the view that the underlying intuitive systems for reasoning about minds and death produce as a default judgment that mental states cease to exist with bodily death.},
  langid = {english},
  keywords = {Afterlife beliefs,Culture and cognition,dissertation,Explicit versus implicit reasoning,Intuitive dualism,Intuitive materialism,Mind-body dualism},
  file = {/Users/suzanne/Zotero/storage/LLZPS5H3/Barrett et al. - 2021 - Intuitive Dualism and Afterlife Beliefs A Cross-C.pdf;/Users/suzanne/Zotero/storage/QZHAJA8R/cogs.html}
}

