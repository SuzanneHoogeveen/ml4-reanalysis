@article{koenig2020religion,
  title = {Religion and Psychiatry: Recent Developments in Research},
  shorttitle = {Religion and Psychiatry},
  author = {Koenig, Harold G. and {Al-Zaben}, Faten and VanderWeele, Tyler J.},
  year = {2020},
  month = sep,
  journal = {BJPsych Advances},
  volume = {26},
  number = {5},
  pages = {262--272},
  publisher = {{Cambridge University Press}},
  issn = {2056-4678, 2056-4686},
  doi = {10.1192/bja.2019.81},
  abstract = {The evidence base on the relationship between religion and mental health is growing rapidly, and we summarise the latest research on the topic. This includes studies on religious involvement and depression, bipolar disorder, suicide, post-traumatic stress disorder (PTSD), substance use disorders, personality disorder, chronic psychotic disorder, marital/family stability, social support and psychological well-being. We also review a relatively new topic in psychiatry, moral injury, which often accompanies PTSD and may interfere with its treatment. We describe a theoretical model that explains how religion might affect mental health and briefly discuss its applications in clinical practice, including a discussion of religiously integrated therapies for depression, anxiety and other emotional problems. Overall, studies indicate that religious involvement often serves as a powerful resource for patients, one that can be integrated into psychiatric care. At times, however, religion may impede or complicate treatment. This article will help clinicians determine, on the basis of the latest research, whether religion is an asset or a liability for a particular patient.},
  langid = {english},
  keywords = {dissertation,mental health,Religion,research,spirituality},
  file = {/Users/suzanne/Zotero/storage/P6Y3VBG8/Koenig et al. - 2020 - Religion and psychiatry recent developments in re.pdf;/Users/suzanne/Zotero/storage/I935VEED/358B30940A36C1CD3AFE7991431BA1A9.html}
}

