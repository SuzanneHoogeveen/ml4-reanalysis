@article{ebert2020religious,
  title = {Religious People Only Live Longer in Religious Cultural Contexts: {{A}} Gravestone Analysis},
  shorttitle = {Religious People Only Live Longer in Religious Cultural Contexts},
  author = {Ebert, Tobias and Gebauer, Jochen E. and Talman, Jildou R. and Rentfrow, P. Jason},
