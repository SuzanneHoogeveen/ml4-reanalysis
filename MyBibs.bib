@article{gligoric2020who,
  title = {``{{Who}} Said It?'' {{How}} Contextual Information Influences Perceived Profundity of Meaningful Quotes and Pseudo-Profound Bullshit},
  shorttitle = {``{{Who}} Said It?},
  author = {Gligori{\'c}, Vuka{\v s}in and Vilotijevi{\'c}, Ana},
  year = {2020},
  journal = {Applied Cognitive Psychology},
  volume = {34},
  number = {2},
  pages = {535--542},
  issn = {1099-0720},
  doi = {10.1002/acp.3626},
  abstract = {Psychological research on pseudo-profound bullshit\textemdash randomly assembled buzz words plugged into a syntactic structure\textemdash has only recently begun. Most such research has focused on dispositional traits, such as thinking styles or political orientation. However, none has investigated contextual factors. In two studies, we introduce a new paradigm by investigating the contextual effect on pseudo-profound bullshit and meaningful quotes. In Study 1, all participants rated the profundity of statements in three contexts: (a) isolated, (b) as allegedly said by a famous author, or (c) within a vignette (short story). Study 2 serves as a conceptual replication in which participants rated statements in only one of three contexts. Overall, our results demonstrate that although contextual information such as author's name increases the perceived profundity of bullshit, it has an inconsistent effect on meaningful quotes. The present study helps to better understand the bullshit receptivity while offering a new line of research.},
  copyright = {\textcopyright{} 2019 The Authors. Applied Cognitive Psychology published by John Wiley \& Sons Ltd.},
  langid = {english},
  keywords = {authorship effect,bullshit,dissertation,labeling effect,pseudo-profound bullshit,quotes},
  file = {/Users/suzanne/Zotero/storage/3LKB8SGH/Gligorić and Vilotijević - 2020 - “Who said it” How contextual information influenc.pdf;/Users/suzanne/Zotero/storage/GQJP6C2U/acp.html}
}

