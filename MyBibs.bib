@article{jaswal2006adults,
  title = {Adults Don't Always Know Best: {{Preschoolers}} Use Past Reliability over Age When Learning New Words},
  shorttitle = {Adults {{Don}}'t {{Always Know Best}}},
  author = {Jaswal, Vikram K. and Neely, Leslie A.},
  year = {2006},
  journal = {Psychological Science},
  volume = {17},
  number = {9},
  pages = {757--758},
  publisher = {{Blackwell Publishing}},
  address = {{United Kingdom}},
  issn = {1467-9280(Electronic),0956-7976(Print)},
  doi = {10.1111/j.1467-9280.2006.01778.x},
  abstract = {Children learn much of what they know--from words to their birth dates to the fact that the earth is round--from what other people tell them. But some people are better informants than others. One way children can estimate the credibility of a speaker is by evaluating how reliable that person has been in the past. Even preschoolers prefer learning new words from an adult who has previously labeled objects correctly rather than one who has labeled objects incorrectly (Koenig, Clement, \& Harris, 2004). Children may also make predictions about a speaker on the basis of that person's membership in a particular group. For example, 4-year-olds expect that an unfamiliar adult, but not necessarily an unfamiliar child, knows the meaning of the word hypochondriac (Taylor, Cartwright, \& Bowden, 1991). Which of these two cues to a speaker's credibility--reliability or age--do 3- and 4-year-old children find more compelling? In this study, when 3- and 4-year-olds had no reason to doubt an adult's credibility, they were more receptive to the novel labels she provided than to those provided by a peer. However, when the adult had mislabeled just four objects, children no longer considered her to be the better informant. Indeed, when the peer had been more reliable than the adult, children actually favored the labels that the peer provided. (PsycINFO Database Record (c) 2016 APA, all rights reserved)},
  keywords = {Credibility,Cues,dissertation,Labeling,Language Development,Stimulus Novelty,Word Meaning,Words (Phonetic Units)},
  file = {/Users/suzanne/Zotero/storage/YBCGNL9K/2006-12459-005.html}
}

